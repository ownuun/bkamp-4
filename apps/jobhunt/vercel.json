{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "installCommand": "cd ../.. && pnpm install",
  "buildCommand": "cd ../.. && pnpm turbo build --filter=jobhunt",
  "crons": [
    {
      "path": "/api/jobs/sync",
      "schedule": "0 */2 * * *"
    },
    {
      "path": "/api/jobs/match-all",
      "schedule": "30 */2 * * *"
    }
  ]
}
